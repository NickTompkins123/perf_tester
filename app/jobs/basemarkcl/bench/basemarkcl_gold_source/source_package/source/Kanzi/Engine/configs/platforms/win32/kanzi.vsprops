<?xml version="1.0" encoding="Windows-1252"?>
<VisualStudioPropertySheet
	ProjectType="Visual C++"
	Version="8.00"
	Name="kanzi"
	OutputDirectory="$(KanziEnginePath)/output/$(PlatformName)/$(ConfigurationName)"
	IntermediateDirectory="$(KanziEnginePath)/output/$(PlatformName)/$(ConfigurationName)/intermediate/$(ProjectName)"
	>
	<Tool
		Name="VCCLCompilerTool"
		AdditionalOptions="/we4002 /we4013 /we4024 /we4028 /we4029 /we4047 /we4113 /we4133 /we4715 /we4716&#x0D;&#x0A;/wd4001 /wd4100 /wd4702"
		AdditionalIncludeDirectories="&quot;$(KanziEnginePath)\libraries\common\libpng\include&quot;;&quot;$(KanziEnginePath)\libraries\common\zlib\include&quot;;&quot;$(KanziEnginePath)\libraries\common\libjpeg\include&quot;;&quot;$(KanziEnginePath)\libraries\common\itype\include&quot;;&quot;$(KanziEnginePath)\libraries\platforms\win32\itype\include&quot;;&quot;$(KanziEnginePath)\libraries\common\freetype\include&quot;;&quot;$(KanziEnginePath)\libraries\common\lua\include&quot;;&quot;$(KanziEnginePath)\libraries\platforms\win32\libjpeg\include&quot;"
		PreprocessorDefinitions="_CRT_SECURE_NO_WARNINGS;WIN32;_CONSOLE;KZ_INLINE=static __inline"
		ExceptionHandling="0"
		DisableLanguageExtensions="true"
		WarningLevel="4"
		CompileAs="1"
	/>
	<Tool
		Name="VCLibrarianTool"
		AdditionalOptions="/ignore:4221"
	/>
	<Tool
		Name="VCLinkerTool"
		AdditionalOptions="/ignore:4221"
		AdditionalDependencies="zlib.lib libpng.lib libjpeg.lib winmm.lib lua5.1.lib tolua_lib.lib freetype.lib"
	/>
	<UserMacro
		Name="KanziEnginePath"
		Value="..\..\.."
		InheritsFromParent="true"
		Delimiter=";"
	/>
</VisualStudioPropertySheet>
